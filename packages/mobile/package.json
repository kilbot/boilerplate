{
  "name": "@boilerplate/mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "ios": "react-native run-ios --simulator=\"iPad Air 2\"",
    "android": "react-native run-android",
    "tsc": "tsc",
    "clean": "shx rm -rf ios/build && rm -rf android/build && rm -rf node_modules && watchman watch-del-all",
    "studio": "open -a /Applications/Android\\ Studio.app ./android/",
    "xcode": "open ios/boilerplate.xcodeproj",
    "postversion": "react-native-version --never-amend"
  },
  "dependencies": {
    "base-64": "0.1.0",
    "events": "3.1.0",
    "pouchdb-adapter-react-native-sqlite": "2.0.0",
    "react-native": "0.62.2",
    "react-native-sqlite-2": "3.0.1",
    "rxdb": "9.2.0"
  },
  "devDependencies": {
    "@types/react-native": "0.62.13",
    "metro-react-native-babel-preset": "0.59.0",
    "react-native-version": "4.0.0"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
